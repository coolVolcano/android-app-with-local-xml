<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#82cc5d" />
      <stroke
          android:width="1dp"
          android:color="#429E34" />
      <corners
          android:radius="1dp" />
      <padding
          android:left="1dp"
          android:top="1dp"
          android:right="1dp"
          android:bottom="1dp" />
    </shape>
  </item>
  <item>
    <shape>
      <solid
          android:color="#99cc80" />
      <stroke
          android:width="1dp"
          android:color="#429E34" />
      <corners
          android:radius="1dp" />
      <padding
          android:left="1dp"
          android:top="1dp"
          android:right="1dp"
          android:bottom="1dp" />
    </shape>
  </item>
</selector>